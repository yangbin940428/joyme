<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
       <!--进度条的背景颜色-->
    <item android:id="@android:id/background" android:drawable="@drawable/audio_seekbar_bg">
    </item>
         <!--网络缓冲第二进度-->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#66ffffff"
                    android:centerColor="#66ffffff"
                    android:centerY="0.75"
                    android:endColor="#66ffffff"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
      <!--已播放进度的颜色-->
    <item android:id="@android:id/progress" android:drawable="@drawable/audio_seekbar_progress">
    </item>

</layer-list>

